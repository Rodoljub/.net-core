  "User": {
    "Password": {
      "RequiredLength": "6"
    },
    "Lockout": {
      "MaxFailedAccessAttempts": "3",
      "DefaultLockoutTimeSpan": "5"
    },
    "Image": {
      "MaxImageSize": "40000"
    },
    "Profile": {
      "MaxUpdateNumber": "30",
      "LastMinutes": "-30"
    }
  },
  "Social": {
    "Facebook": {
      "AppKey": "xxxxxxxxxxxxx",
      "Secret": "xxxxxxxxxxxxxxxxxxxxx"
    },
    "Google": {
      "AppKey": "xxxxxxxxxx",
      "Secret": "xxxxxxxxxxxxxxxxxx"
    }

  },
  "Recaptcha": {
    "SiteKey": "xxxxxxxxxxxxxxxxxxx",
    "SecretKey": "xxxxxxxxxxxxxxxxxxxxxxx"
  },
  "SMTP": {
    "FromAddress": "xxxxxxxxxxxxx",
    "Server": "xxxxxxxxxxxxx",
    "Password": "xxxxxxxxxxxxxxxx"
  },
  "Email": {
    "Sender": {
      "Address": "xxxxxxxxxxxxxx",
      "Name": "OCCPY.COM"
    }
  },
  "Data": {
    "connectionString": "xxxxxx"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Verbose",
        "System": "Verbose"
      }
    },

    "WriteTo:Async": {
      "Name": "Async",
      "Args": {
        "configure": [
          {
            "Name": "RollingFile",
            "Args": {
              "rollingInterval": "Day",
              "rollOnFileSizeLimit": "True",
              "fileSizeLimitBytes": 100000,
              "pathFormat": ".//Logs//{Date}.txt",
              "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}"
            }
          }
        ]
      }
    }
  },
  "Tokens": {
    "Key": "{xxxxxxxxxxxxxxxxxxx}",
    "Issuer": "http://localhost:4260",
    "Audience": "http://localhost",
    "Authority": "http://localhost:4200",
    "Expires": "40",
    "KeepMeLoggednExpires": "7",
    "ValidMaxTameTokenExpiresSoon": "20",
    "BadTokenReturnDateTime": "-1"
  },
  //"CORS": {
  //  "AllowedOrigins": "http://localhost"
