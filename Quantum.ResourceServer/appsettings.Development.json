  //}

  "AllowedHosts": "*",

  "CORS": {
    "AllowedOrigins": [ "http://localhost", "https://localhost" ], // Client URLs
    "AllowedMethods": [ "GET", "POST", "PUT", "PATCH", "DELETE" ]
  },

  "IdentityPages": {
